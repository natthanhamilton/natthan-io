{
  "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "es6":     true,
      "browser": true,
      "node":    true
  },
  "globals": {
      "env": true
  },
    "extends": [
    "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
      "no-unused-vars": 0,
      "max-len": [1, 120, 2, {ignoreComments: true}],
      "no-array-index-key": "off"
    },
 }
